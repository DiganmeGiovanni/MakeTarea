{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Curso</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Materia</th>
                <td>{{ entity.materia }}</td>
            </tr>
            <tr>
                <th>Nomprofesor</th>
                <td>{{ entity.nomProfesor }}</td>
            </tr>
            <tr>
                <th>Apeprofesor</th>
                <td>{{ entity.apeProfesor }}</td>
            </tr>
            <tr>
                <th>Emailprofesor</th>
                <td>{{ entity.emailProfesor }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('curso') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('curso_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
